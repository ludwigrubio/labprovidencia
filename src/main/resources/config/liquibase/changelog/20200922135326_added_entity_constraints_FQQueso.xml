<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity FQQueso.
    -->
    <changeSet id="20200922135326-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="area_id"
                                 baseTableName="fq_queso"
                                 constraintName="fk_fq_queso_area_id"
                                 referencedColumnNames="id"
                                 referencedTableName="area"/>

        <addForeignKeyConstraint baseColumnNames="producto_id"
                                 baseTableName="fq_queso"
                                 constraintName="fk_fq_queso_producto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"/>

        <addForeignKeyConstraint baseColumnNames="analista_id"
                                 baseTableName="fq_queso"
                                 constraintName="fk_fq_queso_analista_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_extra"/>

        <addForeignKeyConstraint baseColumnNames="proveedor_id"
                                 baseTableName="fq_queso"
                                 constraintName="fk_fq_queso_proveedor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personal"/>

    </changeSet>
</databaseChangeLog>
